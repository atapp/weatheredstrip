  "version": "1.0.0",
  "main": "server/weather.js",
  "author": "Greg Hamel <hamegreg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "artoo-js": "^0.4.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "node-fetch": "^2.3.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.3.1",
    "cz-conventional-changelog": "2.1.0"
  },
  "scripts": {
    "commitmsg": "commitlint -E $GIT_PARAMS",
    "cm": "git-cz"
  },
