{
  "name": "Weatheredstrip",
  "description": "A client/server app which provide Aerodrome status by providing METARs/TAFs as well as NOTAMs scrapped from NAV Canada Flight Planning webpages.",
  "version": "1.0.0",
  "main": "server/weather.js",
  "author": "Greg Hamel <hamegreg@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.3.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1"
  },
  "scripts": {
    "start": "node server/weather.js",
    "cm": "git-cz",
    "pushit": "git push origin master",
    "deploy": "git push deploy master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
